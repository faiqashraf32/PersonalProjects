// searching for a specified value in coral (linear search)

integer array(5) A
integer key
integer i
integer flag

i = 0
while i < 5
   A[i] = Get next input
   i = i + 1
   
key = Get next input
i = 0
while i < 5
   if A[i] == key
      Put "Found it!" to output
      flag = 1
   i = i + 1
if flag == 0
   Put "No match found" to output